class Solution {
    int missingNum(int arr[]) {
        // code here
      
         int[] arr1=new int[arr.length+2];
         
         for(int i=0;i<arr.length;i++)
         {
             arr1[arr[i]]=1;
         }
         
         int res=0;
         for(int i=0;i<arr1.length;i++)
         {
         if(arr1[i]==0)
         {
             res=i;
         }
         }
         return res;
         
    }
}


//O(2n)
//O(n)
