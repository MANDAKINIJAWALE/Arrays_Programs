class Solution {
    public int removeDuplicates(int[] arr) {

     int j=0;
      for(int i=1;i<arr.length;i++)
      {
             if(arr[j]!=arr[i])
             {
               
                arr[j+1]=arr[i];
                 j++;
             }
      }
        return j+1;
    }
}

//O(n)
//O(1)
