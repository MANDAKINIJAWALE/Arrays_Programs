class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        
        int j=0;
        
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]!=0)
            {
                int t=arr[j];
                arr[j]=arr[i];
                arr[i]=t;
                j++;
                
            }
        }
    }
}

//O(n)
//O(1)
